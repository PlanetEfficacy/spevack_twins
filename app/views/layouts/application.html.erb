<!DOCTYPE html>
<html>
  <head>
    <title>SpevackTwinsRailsReact</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <ul>
      <% if user_signed_in? %>
        <li><<%= link_to 'Sign out', destroy_user_session_path, method: delete %>/li>
      <% else %>
    </ul>
    <%= yield %>
  </body>
</html>
